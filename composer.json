{
    "name": "tenolo/entity-bundle",
    "description": "Add basic entity functions.",
    "keywords": [
        "entity",
        "bundle",
        "symfony",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "https://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0",
        "symfony/config": "~3.4|~4.0",
        "symfony/http-foundation": "~3.4|~4.0",
        "symfony/options-resolver": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",

        "doctrine/common": "~2.6",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.6",

        "tenolo/utilities": "~1.2",

        "nesbot/carbon": "~2.17",
        "sonata-project/doctrine-extensions": "~1.0",
        "rikbruil/doctrine-specification": "~1.1",
        "austinsmorris/doctrine-dbal-datetimeutc": "~2.0",
        "stof/doctrine-extensions-bundle": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\EntityBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
