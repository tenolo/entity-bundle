{
    "name": "tenolo/entity-bundle",
    "description": "Add basic entity functions.",
    "keywords": [
        "entity",
        "bundle",
        "symfony",
        "tenolo"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "nl@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/options-resolver": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",

        "doctrine/common": "~2.6",
        "doctrine/orm": "~2.5",

        "tenolo/utilities": "~1.2",

        "austinsmorris/doctrine-dbal-datetimeutc": "~2.0",
        "stof/doctrine-extensions-bundle": "~1.2",
        "mmoreram/symfony-bundle-dependencies": "~1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\EntityBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\EntityBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/3.0": "3.0-dev",
            "dev-release/2.0": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
